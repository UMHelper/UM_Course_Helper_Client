/* pages/search/search.wxss */

page{
    position: relative;
    background:#9eb3c2;
    margin: 0;
}
.logo_contain{   
    width: 600rpx;
    margin-left: 100rpx;
    margin-top: 50rpx;
}

.logo_contain .logo{
    display: inline-block;
    width: 250rpx;
    height: 250rpx;
    vertical-align:middle;
    /* box-shadow: 0px 0px 68px -12px rgba(69,69,69,1); */
    /* -webkit-box-shadow: 0px 0px 68px -12px rgba(69,69,69,1); */
    border-radius: 250rpx;
}
.logo_contain .logo image{
    width: 100%;
    height: 100%;
    
}

.logo_contain .text{
    color: #fff;
    margin-left: 50rpx;
    font-size: 50rpx;
    display: inline-block;
    vertical-align:middle;
}

.logo_contain .text view{
    line-height: 70rpx;
    margin:auto ;
}

.search_contain{
    position: relative;
    background-color:#fff ;
    width: 600rpx;
    height: 600rpx;
    margin: auto;
    margin-top: 80rpx;
    border-radius: 60rpx;
    box-shadow: 0px 0px 64px  rgba(0,0,0,.46);
    -webkit-box-shadow: 0px 0px 64px  rgba(0,0,0,.46);

}

.search_contain .search{
    position: absolute;
    width: 80%;
    height: 90rpx;
    /* background-color: #5d93ab; */
    background-color: #fff;
    border-radius: 45rpx;
    left: 10%;
    top: 25%;
    border: 3rpx #5d93ab solid;

}

.search_contain .search input{
    font-size: 40rpx;
    width: 75%;
    height: 100%;
    margin: auto;
    /* background-color: #5d93ab; */
    background-color: #fff;
    border-radius: 15rpx;
    color: #000;
    /* border: 3rpx #5d93ab solid; */
}

.search_contain .submit{
    position: absolute;
    left: 10%;
    bottom: 25%;
    width: 80%;
    height: 90rpx;
}

.search_contain .submit button{
    width: 100%;
    height: 100%;
    background: #9eb3c2;
    border-radius: 45rpx;
}

.search_contain .submit button view{
    margin: auto;
    width: 80%;
    font-size: 40rpx;
    color: #fff;
    height: 90rpx;
}


.info_contain .info{
    color: #fff;
    display: flex;
    justify-content: center;
    margin-top: 100rpx;
}
.info_contain view{
    display: inline;
}
